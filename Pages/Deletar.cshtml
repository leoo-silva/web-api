@page
@using web.Entities.Pessoa.DAO;
@using web.Entities.Pessoa.Model;


@{
    PessoaDAO dao;
    try
    {
        string cpf = Request.Query["cpf"];

        dao = new PessoaDAO();
        dao.DeletaDados(cpf);
    }
    catch (Exception erro)
    {
        throw new Exception("Error: " + erro.Message);
    }

    Response.Redirect("/Listagem");
}
